        //20190920 Init IndexDB Bruce Chuang
        InitDb: function() {
            var a3db = null;
            var req = indexedDB.open("a3", 1);



            //this.patientdiagnoses = "heart";

            //req.onsuccess = this.onSuccess();


            req.onsuccess = function(evt) {

                //this.onSuccess(evt);

                console.log("req.onSucess");
                // a3db = req.result;
                //this.$emit(req.result, this.$a3dbb);
                this.$a3dbb = req.result;

                //a3db = this.result;

                //-------------------------------------------------------------
                //20190926 init DB 讀取index DB Bruce Chuang
                var tx = this.$a3dbb.transaction(["a3"], "readwrite");
                var objectStore = tx.objectStore(["a3"]);

                var readq = objectStore.openCursor();


                readq.onsuccess = function(evt) {

                    //var cursor = evt.target.result;

                    var cursor = null;

                    if (cursor) {

                        console.log("displayPubList cursor:", cursor);


                        /*
                        this.$indexDBdata = cursor.value;

                        this.patientId = this.$indexDBdata.patientID;
                        this.patientName = this.$indexDBdata.patientname;
                        this.patientgender = this.$indexDBdata.patientgender;
                        this.patientbirthday = this.$indexDBdata.patientbirthday;
                        this.patientdiagnoses = this.$indexDBdata.patientdiagnoses;
                        this.patientallergyintolerance = this.$indexDBdata.patientallergyintolerance;
                        this.patientlocation = this.$indexDBdata.patientlocation;
                        this.patienthistory = this.$indexDBdata.patienthistory;
                        this.patientrighteye = this.$indexDBdata.patientrighteye;
                        this.patientlefteye = this.$indexDBdata.patientlefteye;
                        this.Nasalairway = this.$indexDBdata.Nasalairway;
                        this.Oralairway = this.$indexDBdata.Oralairway;
                        this.NCcheckbox = this.$indexDBdata.NCcheckbox;
                        this.NCdata = this.$indexDBdata.NCdata;
                        this.Simplemaskcheckbox = this.$indexDBdata.Simplemaskcheckbox;
                        this.SimpleMaskdata = this.$indexDBdata.SimpleMaskdata;
                        */
                        /*
                                    req2 = objectStore.get(cursor.key);

                                    req2.onsuccess = function(evt) {
                                      var value = evt.target.result;

                                      console.log(cursor.key);
                                    };
                                    */

                        cursor.continue();
                        //i++;
                    } else {
                        console.log("No more entries");
                    }
                };
                //-------------------------------------------------------------

                console.log("Index Db OPen");
            };



            req.onerror = function(evt) {};

            req.onupgradeneeded = function(evt) {
                var db = evt.target.result;

                console.log("req.onupgradeneeded");

                var store = db.createObjectStore("a3", {
                    keyPath: "id",
                    autoIncrement: true
                });

                store.createIndex("ReferralID", "ReferralID", { unique: true });
                store.createIndex("patientID", "patientID", { unique: true });
                store.createIndex("patientname", "patientname", { unique: false });
                store.createIndex("patientgender", "patientgender", { unique: false });
                store.createIndex("patientbirthday", "patientbirthday", {
                    unique: false
                });
                store.createIndex("patientdiagnoses", "patientdiagnoses", {
                    unique: false
                });
                store.createIndex(
                    "patientallergyintolerance",
                    "patientallergyintolerance", { unique: false }
                );
                store.createIndex("patientlocation", "patientlocation", {
                    unique: false
                });
                store.createIndex("patienthistory", "patienthistory", {
                    unique: false
                });
                store.createIndex("patientrighteye", "patientrighteye", {
                    unique: false
                });
                store.createIndex("patientlefteye", "patientlefteye", {
                    unique: false
                });
                store.createIndex("Nasalairway", "Nasalairway", { unique: false });
                store.createIndex("Oralairway", "Oralairway", { unique: false });
                store.createIndex("NCcheckbox", "NCcheckbox", { unique: false });
                store.createIndex("NCdata", "NCdata", { unique: false });
                store.createIndex("Simplemaskcheckbox", "Simplemaskcheckbox", {
                    unique: false
                });
                store.createIndex("SimpleMaskdata", "SimpleMaskdata", {
                    unique: false
