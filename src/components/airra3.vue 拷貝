        DrawPeople: function() {



            const image = new Image();

            image.src =
                "data:image/png;base64,"; //20190312 Bruce;

            image.onload = function() {

                var c = document.getElementById("PeopleShow");

                if (c !== undefined && c !== null) {
                    var ctx2 = c.getContext("2d");

                    ctx2.drawImage(image, 0, 0, 500, 350);
                    console.log("image lonad");
                }
            };
        },
        DrawToTable: function() {

            console.log("DrawToTable()");

            //------------------------------------------------------------
            var c = document.getElementById("TranCanvas");
            var ctx = c.getContext("2d");
            ctx.clearRect(0, 0, c.width, c.height);
            if (this.TranShowCanvas !== null)
                ctx.drawImage(this.TranShowCanvas, 0, 0, 200, 200);
            //------------------------------------------------------------
            var c2 = document.getElementById("ReciveCanvas");
            var ctx2 = c2.getContext("2d");
            ctx2.clearRect(0, 0, c2.width, c2.height);
            if (this.ReciveShowCanvas !== null)
                ctx2.drawImage(this.ReciveShowCanvas, 0, 0, 200, 200);
            //------------------------------------------------------------
            var c3 = document.getElementById("PatientCanvas");
            var ctx3 = c3.getContext("2d");
            ctx3.clearRect(0, 0, c3.width, c3.height);

            if (this.PatientShowCanvas !== null)
                ctx3.drawImage(this.PatientShowCanvas, 0, 0, 200, 200);
            //------------------------------------------------------------
            var c4 = document.getElementById("TeachCanvas");
            var ctx4 = c4.getContext("2d");
            ctx4.clearRect(0, 0, c4.width, c4.height);
            if (this.TechSignShowCanvas !== null)
                ctx4.drawImage(this.TechSignShowCanvas, 0, 0, 200, 200);
            //------------------------------------------------------------
            var c7 = document.getElementById("imagePeople");
            if (c7 !== undefined && c7 !== null) {

                var ctx7 = c7.getContext("2d");
                ctx7.clearRect(0, 0, c7.width, c7.height);

                if (this.PeopleShowCanvas !== null)
                    ctx7.drawImage(this.PeopleShowCanvas, 0, 0, 500, 400);
            }
            //------------------------------------------------------------

        },
